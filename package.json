{
  "name": "tulipbookshop",
  "version": "0.1.0",
  "description": "Front End to Tulip Bookshop",
  "scripts": {
    "js": "webpack --watch",
    "css": "postcss -c postcss.config.js --watch",
    "copy-static": "cp src/index.html build/ && cp -r src/assets build/",
    "compile": "webpack && postcss -c postcss.config.js && npm run copy-static",
    "start": "webpack-dev-server"
  },
  "license": "",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.5",
    "firebase": "^3.4.0",
    "lodash": "^4.13.1",
    "postcss": "^5.0.21",
    "postcss-cli": "^2.5.2",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-mixin": "^3.0.5",
    "reactfire": "^1.0.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0"
  }
}
